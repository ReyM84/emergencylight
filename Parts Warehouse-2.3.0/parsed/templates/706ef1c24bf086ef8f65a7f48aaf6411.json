{
  "components/common/quick-search": "<div class=\"container\">\n  {{{ snippet \"forms_search\"}}}\n  <form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n      <div class=\"form-field\">\n        <label class=\"u-hiddenVisually\" for=\"search_query\"\n          >{{lang \"search.quick_search.input_label\"}}</label\n        >\n        <input\n          class=\"form-input\"\n          data-search-quick\n          name=\"search_query\"\n          id=\"search_query\"\n          data-error-message=\"{{lang 'search.error.empty_field'}}\"\n          placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n          autocomplete=\"off\"\n        />\n        <div class=\"search-icon\">\n          <i class=\"fa fa-search\" aria-hidden=\"true\"></i>\n        </div>\n        <input\n          class=\"button button--primary form-prefixPostfix-button--postfix\"\n          type=\"submit\"\n          value=\"\"\n        />\n      </div>\n    </fieldset>\n    <div class=\"clear\"></div>\n  </form>\n  <!-- <a href=\"#\" class=\"reset quicksearch\" aria-label=\"reset search\" role=\"button\">\n    <span class=\"reset\" aria-hidden=\"true\">Reset Search</span>\n  </a> -->\n  <a\n    href=\"#\"\n    class=\"modal-close custom-quick-search\"\n    aria-label=\"{{lang 'common.close'}}\"\n    role=\"button\"\n  >\n    <span aria-hidden=\"true\">&#215;</span>\n  </a>\n  <!-- <section class=\"quickSearchResults\" data-bind=\"html: results\">\n  </section> -->\n</div>\n"
}